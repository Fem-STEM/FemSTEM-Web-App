        function showFullStory(btnId)
        {
          document.querySelectorAll(".story-hid")[btnId].style.display = "inline-block";
          document.querySelectorAll(".story-btn")[btnId].style.display = "none";
        }

        function hideullStory(btnId)
        {
          document.querySelectorAll(".story-hid")[btnId-2].style.display = "none";
          document.querySelectorAll(".story-btn")[btnId-2].style.display = "block";
